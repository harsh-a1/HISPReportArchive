            <xml-property name="queryText"><![CDATA[select drugname,categry,formname,dozage,categorynaem ,openbal ,closbal,dd,ff,trans,isp,isa,typetrans from
(SELECT id.name as drugname,id.category_id as categry,idf.name as formname,idf.dozage as dozage,ic.name as categorynaem, i.`opening_balance` as openbal, 
i.closing_balance as closbal,i.`drug_id` as 'dd', i.`formulation_id` as 'ff',idt.id as trans,
case when idt.description like "%ISSUE DRUG TO PATIENT %" then sum(i.issue_quantity) end as 'isp',
case when idt.description like "%ISSUE DRUG TO ACCOUNT%" then sum(i.issue_quantity )end as 'isa',
case when idt.description like "%ISSUE DRUG TO PATIENT %" then idt.description else
case when idt.description like "%ISSUE DRUG TO ACCOUNT%"  then idt.description end end as typetrans ,ur.role,idt.created_by 
FROM inventory_store_drug_transaction_detail i
inner join inventory_store_drug_transaction idt on i.transaction_id=idt.id
inner join inventory_drug id on i.drug_id=id.id
inner join inventory_drug_formulation idf on i.formulation_id=idf.id
inner join inventory_drug_category ic on id.category_id=ic.id
inner join person_name pn on pn.given_name=idt.created_by 
inner join users us on  us.person_id=pn.person_id
inner join user_role ur on ur.user_id=us.user_id and ur.role like "%Pharmacy%"
where date(i.created_on) between ? and ? 
group by i.`drug_id`, i.`formulation_id`,idt.description
)amb where typetrans is not null

]]></xml-property>
