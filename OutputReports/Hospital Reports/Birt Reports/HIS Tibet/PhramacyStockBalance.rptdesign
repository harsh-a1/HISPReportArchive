            <xml-property name="queryText"><![CDATA[SELECT id.name,id.category_id,idf.name,idf.dozage,ic.name,i.`id`, i.`transaction_id`, i.`drug_id`, i.`formulation_id`,i.`opening_balance`, i.closing_balance,
sis.isp,sis.isa,sis.trans FROM inventory_store_drug_transaction_detail i
INNER JOIN inventory_store_drug_transaction idt ON i.transaction_id=idt.id
INNER JOIN inventory_drug id ON i.drug_id=id.id
INNER JOIN inventory_drug_formulation idf ON i.formulation_id=idf.id
INNER JOIN inventory_drug_category ic ON id.category_id=ic.id
INNER JOIN (SELECT i.drug_id AS 'did',i.formulation_id AS 'form',MAX(i.created_on) AS maxd
FROM inventory_store_drug_transaction_detail i
INNER JOIN inventory_store_drug_transaction idt ON i.transaction_id=idt.id
INNER JOIN person_name pn ON pn.given_name=idt.created_by 
INNER JOIN users us ON  us.person_id=pn.person_id
INNER JOIN user_role ur ON ur.user_id=us.user_id
WHERE ur.role LIKE "%Pharmacy%"
GROUP BY i.drug_id,i.formulation_id) ph ON i.drug_id=ph.did AND i.`formulation_id`=ph.form AND i.created_on=ph.maxd
LEFT JOIN (SELECT  i.`drug_id` AS 'dd', i.`formulation_id` AS 'ff',idt.id AS trans,
CASE WHEN idt.description LIKE "%ISSUE DRUG TO PATIENT %" THEN SUM(i.issue_quantity) END AS 'isp',
CASE WHEN idt.description LIKE "%ISSUE DRUG TO ACCOUNT%" THEN SUM(i.issue_quantity )END AS 'isa',
CASE WHEN idt.description LIKE "%ISSUE DRUG TO PATIENT %" THEN idt.description ELSE
CASE WHEN idt.description LIKE "%ISSUE DRUG TO ACCOUNT%"  THEN idt.description END END AS typetrans 
FROM inventory_store_drug_transaction_detail i
INNER JOIN inventory_store_drug_transaction idt ON i.transaction_id=idt.id
INNER JOIN inventory_drug id ON i.drug_id=id.id
INNER JOIN person_name pn ON pn.given_name=idt.created_by 
INNER JOIN users us ON  us.person_id=pn.person_id
INNER JOIN user_role ur ON ur.user_id=us.user_id AND ur.role LIKE "%Pharmacy%"
WHERE DATE(i.created_on) BETWEEN ? AND ?
GROUP BY i.`drug_id`, i.`formulation_id`,idt.description) sis ON i.drug_id=sis.dd AND i.formulation_id=sis.ff
INNER JOIN person_name pn ON pn.given_name=idt.created_by 
INNER JOIN users us ON  us.person_id=pn.person_id
INNER JOIN user_role ur ON ur.user_id=us.user_id 
WHERE ur.role LIKE "%Pharmacy%"
GROUP BY id.category_id,i.`drug_id`, i.`formulation_id`,sis.trans]]></xml-property>
